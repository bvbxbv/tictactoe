{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES6",
		"baseUrl": "./src",
		"paths": {
			"@assets/*": ["assets/*"],
			"@pipes/*": ["pipes/*"],
			"@errors/*": ["errors/*"],
			"@controllers/*": ["controllers/*"],
			"@models/*": ["models/*"],
			"@views/*": ["views/*"],
			"@configs/*": ["configs/*"],
			"@core/*": ["core/*"],
			"@utils/*": ["utils/*"],
			"@factories/*": ["core/factories/*"]
		},
		"checkJs": true
	},
	"exclude": ["node_modules"]
}
